{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react", 
    "noImplicitAny": false,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "types": [ "webpack-env" ],
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
